{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run frontend\" \"npm run backend\"",
    "dev:alt": "npm-run-all --parallel frontend backend",
    "frontend": "cross-env NODE_ENV=development vite",
    "backend": "npm --prefix server start",
    "start-server": "npm --prefix server start",
    "build": "cross-env NODE_ENV=production vite build",
    "build:windows": "set NODE_ENV=production && vite build",
    "build:unix": "NODE_ENV=production vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "cross-env NODE_ENV=test vitest --environment jsdom",
    "test:watch": "cross-env NODE_ENV=test vitest --environment jsdom --watch",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:windows": "rmdir /s /q dist && rmdir /s /q node_modules\\.vite",
    "postinstall": "npm --prefix server install"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@types/qrcode": "^1.5.5",
    "bcryptjs": "^3.0.2",
    "caniuse-lite": "^1.0.30001737",
    "date-fns": "^4.1.0",
    "framer-motion": "^7.10.3",
    "html5-qrcode": "^2.3.8",
    "lucide-react": "^0.344.0",
    "mongodb": "^6.19.0",
    "node-fetch": "^3.3.2",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^7.8.2",
    "xlsx": "^0.18.5",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "focus-trap-react": "^9.0.0",
    "globals": "^15.9.0",
    "jsdom": "^27.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  }
}
